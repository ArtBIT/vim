"------------------------------------------------------------------------------
" CUSTOM VIM CONFIG  
"------------------------------------------------------------------------------
if filereadable(".vimrc")
    so .vimrc
endif


"------------------------------------------------------------------------------
" Clean up HAR copied from browser
"------------------------------------------------------------------------------
fu! CleanupHAR() 
    exe '%s/http/\rhttp/g'
    exe 'g!/^http[s]\?:/d'
    exe '%s/".*//g'
    normal ggVG
endf
